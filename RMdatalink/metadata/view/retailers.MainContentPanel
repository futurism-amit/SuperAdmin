                                            "                          }\r",
                                            "\r",
                                            "                     }\r",
                                            "                      return \"\" ;\r",
                                            "                   },\r",
                                            "\r",
                                            "                   getCity: function(value){\r",
                                            "                       if(value.locations){\r",
                                            "\r",
                                            "                           if(value.locations[0]){\r",
                                            "\r",
                                            "                               if(value.locations[0].city!== \"\"){\r",
                                            "                                   return value.locations[0].city;\r",
                                            "                               }\r",
                                            "\r",
                                            "\r",
                                            "                           }\r",
                                            "                       }\r",
