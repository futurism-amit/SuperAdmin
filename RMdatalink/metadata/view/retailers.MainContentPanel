                                            "                     if(cmpContacts){\r",
                                            "\r",
                                            "                         for(var i=0; i < cmpContacts.length ; i++ ){\r",
                                            "\r",
                                            "                             if(cmpContacts[i].is_account_mgr){\r",
                                            "\r",
