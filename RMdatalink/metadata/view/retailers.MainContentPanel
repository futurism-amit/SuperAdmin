                                            "\r",
                                            "                     var cmpContacts = value.company_contacts ;\r",
                                            "                     if(cmpContacts){\r",
                                            "\r",
                                            "                         for(var i=0; i < cmpContacts.length ; i++ ){\r",
                                            "\r",
                                            "                             if(cmpContacts[i].is_account_mgr){\r",
                                            "\r",
                                            "                                 return cmpContacts[i].companycontact_firstname +\" \"+ cmpContacts[i].companycontact_lastname ;\r",
                                            "                             }\r",
                                            "                         }\r",
                                            "\r",
                                            "                         if(cmpContacts[0]){\r",
                                            "\r",
                                            "                                 return cmpContacts[0].companycontact_firstname +\" \"+ cmpContacts[0].companycontact_lastname ;\r",
                                            "                         }\r",
                                            "\r",
                                            "                     }\r",
                                            "                      return \"\" ;\r",
                                            "                   },\r",
                                            "\r",
                                            "                    getAccMgrPhone: function(value){\r",
                                            "\r",
                                            "                     var cmpContacts = value.company_contacts ;\r",
                                            "                     if(cmpContacts){\r",
                                            "\r",
                                            "                         for(var i=0; i < cmpContacts.length ; i++ ){\r",
                                            "\r",
                                            "                             if(cmpContacts[i].is_account_mgr){\r",
