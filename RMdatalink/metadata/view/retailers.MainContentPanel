                                            "                       {\r",
                                            "                           switch(value[key]){\r",
                                            "\r",
                                            "                               case \"ACTIVE\":\r",
                                            "                                   return '#39b3d7';\r",
                                            "                               case \"INACTIVE\":\r",
                                            "                                   return '#ebebeb';\r",
                                            "                               case \"PROSPECT\":\r",
                                            "                                   return '#47a447';\r",
                                            "                               case \"HOT_PROSPECT\":\r",
                                            "                                   return '#ed9c28';\r",
                                            "                               case \"PENDING\":\r",
                                            "                                   return '#3276b1';\r",
                                            "                           }\r",
                                            "\r",
                                            "\r",
                                            "                           return '#2EC1DA';\r",
                                            "                       }\r",
                                            "                       else\r",
                                            "                           return '#ebebeb';\r",
                                            "\r",
                                            "                   }\r",
                                            "\r",
                                            "               },\r",
                                            "\r",
                                            "               {\r",
                                            "                   setOwedMonthlyField: function(rec){\r",
                                            "                       var value = 0 ;\r",
                                            "                       if(rec.store_products.rmpro_status && rec.store_products.rmpro_status == \"ACTIVE\"){\r",
                                            "\r",
                                            "                           value += parseFloat(rec.product_billng.product_rmpro.monthly_membership,0) ;\r",
                                            "                       }\r",
                                            "\r",
                                            "                       if(rec.store_products.datalink_status && rec.store_products.datalink_status == \"ACTIVE\"){\r",
                                            "\r",
                                            "                           value += parseFloat(rec.product_billng.product_datalink.monthly_membership,0) ;\r",
                                            "                       }\r",
                                            "\r",
                                            "                        value.toFixed(2);\r",
                                            "\r",
                                            "\r",
                                            "\r",
                                            "                       if(value === 0)\r",
                                            "                           return \"0.00\";\r",
                                            "                       else\r",
                                            "                       {\r",
                                            "                           var sumStr = \"\"+ value;\r",
                                            "                           var sumStrSplit0 = sumStr.split('.')[0];\r",
                                            "\r",
                                            "                           var len = sumStrSplit0.length;\r",
                                            "                           if(len > 3)\r",
                                            "                           {\r",
                                            "                               sumStr1 = sumStrSplit0.substr(0,(len-3));\r",
                                            "                               sumStr2 = sumStrSplit0.substr((len-3),(len-1));\r",
                                            "                               sumStrSplit0 =  sumStr1 + ',' + sumStr2 ;\r",
                                            "                           }\r",
                                            "\r",
                                            "                           if(sumStr.split('.')[1])\r",
                                            "                               return sumStrSplit0 + '.' +sumStr.split('.')[1];\r",
                                            "                           else\r",
                                            "                           {\r",
                                            "                               return  sumStrSplit0;\r",
                                            "                           }\r",
                                            "                       }\r",
                                            "\r",
                                            "\r",
                                            "                   }\r",
