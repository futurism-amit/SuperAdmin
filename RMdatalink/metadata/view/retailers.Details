                                                                                                    "managerUSers.tu_relationsheep_details.rd_wife=    Ext.ComponentQuery.query('#RDUserProfileWife')[0].getValue();\r",
                                                                                                    "managerUSers.tu_relationsheep_details.rd_relatives=    Ext.ComponentQuery.query('#RDUserProfileRelatives')[0].getValue();\r",
