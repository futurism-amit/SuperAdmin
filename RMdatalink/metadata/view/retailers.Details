                                                        "fn": "onRDManagerFormPanelInitialize",
                                                        "implHandler": [
                                                            "var fileField = component.down('#RDManagerAddPhoto');\r",
                                                            "var fileSelect = fileField.element.dom.querySelector('input[type=file]');\r",
                                                            "\r",
                                                            "fileSelect.onchange = function(me,value,startValue) {\r",
                                                            "\r",
                                                            "      Ext.Msg.alert('Alert.', 'Currently image uploading is not available.', Ext.emptyFn);\r",
                                                            "    return ;\r",
                                                            "    var file = this.files[0];\r",
                                                            "    var imageType = /image.*/;\r",
                                                            "    if (!file.type.match(imageType)) {\r",
                                                            "      return;\r",
                                                            "    }\r",
                                                            "    var reader = new FileReader();\r",
                                                            "    reader.onload = function(event) {\r",
                                                            "        var dataUri = event.target.result;\r",
                                                            "        component.down('#RDManagerAddPhotoImage').setSrc(dataUri).setHidden(false);\r",
                                                            "        fileField.setHidden(true);\r",
                                                            "    };\r",
                                                            "    reader.onerror = function(event) {\r",
                                                            "        console.error(\"File could not be read! Code \" + event.target.error.code);\r",
                                                            "    };\r",
                                                            "    reader.readAsDataURL(file);\r",
                                                            "};"
