                                                                        "fn": "onRDCRMNotesPhotosAddBtnTap",
                                                                        "implHandler": [
                                                                            "var fileSelect = document.createElement(\"input\");\r",
                                                                            "fileSelect.type = \"file\";\r",
                                                                            "fileSelect.accept = \"image/*\";\r",
                                                                            "fileSelect.onchange = function() {\r",
                                                                            "\r",
                                                                            "    var file = this.files[0];\r",
                                                                            "    var imageType = /image.*/;\r",
                                                                            "    if (!file.type.match(imageType)) {\r",
                                                                            "      return;\r",
                                                                            "    }\r",
                                                                            "    var photoList = button.parent.parent.down('#RDCRMNotesPhotosView');\r",
                                                                            "    var photoStore = photoList.getStore();\r",
                                                                            "\r",
                                                                            "    var reader = new FileReader();\r",
                                                                            "    reader.onload = function(event) {\r",
                                                                            "        var dataUri = event.target.result;\r",
                                                                            "        photoStore.add({\r",
                                                                            "            imagename:file.name,\r",
                                                                            "            image:dataUri\r",
                                                                            "        });\r",
                                                                            "    };\r",
                                                                            "    reader.onerror = function(event) {\r",
                                                                            "        console.error(\"File could not be read! Code \" + event.target.error.code);\r",
