                                                                                                    "            list.setData(managerUSers[i].tu_access_rights);\r",
                                                                                                    "        }\r",
                                                                                                    "        else {\r",
                                                                                                    "            list.setData([\r",
                                                                                                    "                {\r",
                                                                                                    "                    \"right_name\": \"Access Data and Images\",\r",
                                                                                                    "                    \"right_status\": false\r",
                                                                                                    "                },\r",
