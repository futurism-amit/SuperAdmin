                                                                                                    "\r",
                                                                                                    "    var activeIndex = RMdatalink.app.getController('RetailerDeatilsDataSet').config.activeIndex ;\r",
                                                                                                    "\r",
                                                                                                    "    var managerUSers ;\r",
                                                                                                    "\r",
                                                                                                    "    if(vendorREPsDetails.getCount() === activeIndex)\r",
                                                                                                    "    {\r",
                                                                                                    "        haveToAdd = 1 ;\r",
                                                                                                    "        managerUSers = {\r",
                                                                                                    "            tu_relationsheep_details :{\r",
                                                                                                    "\r",
                                                                                                    "            }\r",
