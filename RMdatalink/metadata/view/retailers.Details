                                                                                                    "\r",
                                                                                                    "\r",
                                                                                                    "function fillUserRelationData(selectedUserRecord,index){\r",
                                                                                                    "    var userData = selectedUserRecord.record.data ;\r",
                                                                                                    "      var managerUSers =userData.manager_teamusers[index] ;\r",
                                                                                                    "\r",
