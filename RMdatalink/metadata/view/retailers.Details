                                                                                                    "                    \"right_name\": \"Repricing\",\r",
                                                                                                    "                    \"right_status\": false\r",
                                                                                                    "                },\r",
                                                                                                    "                {\r",
                                                                                                    "                    \"right_name\": \"Export\",\r",
                                                                                                    "                    \"right_status\": false\r",
