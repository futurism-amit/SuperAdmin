                    "      Ext.Viewport.setMasked(false);\r",
                    "}\r",
                    "\r",
                    "function rollBackStorePageNo(){\r",
                    "    var activeListStore = null ;\r",
                    "      switch(store_status){\r",
                    "        case \"ACTIVE\":\r",
                    "\r",
                    "            activeListStore = Ext.ComponentQuery.query('#usersTab')[0].down('#retailerList').getStore();\r",
                    "\r",
                    "            break;\r",
                    "        case \"PROSPECTS\":\r",
                    "\r",
