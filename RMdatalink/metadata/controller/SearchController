                    "\r",
                    "        var searchQuery =  that.getSearchQuery(store_status) ;\r",
                    "         var tquery = {\r",
                    "             '$and':\r",
                    "             [\r",
                    "\r",
                    "            searchQuery\r",
                    "\r",
