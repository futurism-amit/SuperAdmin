                    "     RMdatalink.iwa.rdl.queryDB(\r",
                    "         {\r",
                    "          collection: dbEnv + \"rdl_invoice_history\",\r",
                    "          pageNo: 1 ,\r",
                    "          pageSize: -1 ,\r",
                    "          sortBy:{invoice_number:1/-1},\r",
                    "         query:{invoice_number: { $exists: true}           },\r",
                    "         fields:{invoice_number:1},\r",
                    "         isDeletedRecords:true},success,error);\r",
                    "\r",
                    "    var invoice_number = 9999 ;\r",
                    "    function success(){\r",
                    "\r",
                    "        invoice_number = arguments[0].items[0].invoice_number ;// parseInt(arguments[0].items[0].account_no,1) ;\r",
                    "\r",
                    "        var temp = parseInt(invoice_number) ;\r",
                    "        temp = temp + 1 ;\r",
                    "\r",
                    "        callback(temp);\r",
                    "    }\r",
                    "\r",
                    "    function error(){\r",
                    "\r",
                    "        Ext.Viewport.setMasked(false) ;\r",
                    "        aler(\"Error in invoice no  generation\");\r",
                    "\r",
                    "    }\r",
