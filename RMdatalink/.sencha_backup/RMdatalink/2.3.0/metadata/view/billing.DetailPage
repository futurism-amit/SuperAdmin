                                                            "    var rmProInvoicePanel = Ext.ComponentQuery.query('#productRmproInvoicePanel')[0] ;\r",
                                                            "\r",
                                                            "    if(! rmProInvoicePanel.down('#invoiceAmmountFld').getValue() || rmProInvoicePanel.down('#invoiceAmmountFld').getValue() <= 0){\r",
                                                            "\r",
                                                            "        Ext.Msg.alert(\"Alert\",\"Please Enter Ammount.\");\r",
                                                            "        return ;\r",
                                                            "    }\r",
                                                            "\r",
                                                            "    if(! rmProInvoicePanel.down('#invoiceBalanceDueFld').getValue() || rmProInvoicePanel.down('#invoiceBalanceDueFld').getValue() <= 0 ){\r",
                                                            "\r",
                                                            "\r",
                                                            "          RMdatalink.app.getController('InvoiceController').config.isSalesCommissionForRmPro = true ;\r",
                                                            "          RMdatalink.app.getController('InvoiceController').saveRmProInvoice(true,\"product_rmpro\");\r",
