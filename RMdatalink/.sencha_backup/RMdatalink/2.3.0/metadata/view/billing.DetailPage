                                        "implHandler": [
                                            "\r",
                                            "var headers = component.down(\"#headerList\");\r",
                                            "headers.setData([{}]);\r",
                                            "headers.setItemTpl(\r",
                                            "    Ext.create('Ext.XTemplate',\r",
                                            "               '<div style=\"font-weight: bold;  background-color: gainsboro;\" class=\"x-rm-listtpl-main onlyBottomBorderDiv\">',\r",
                                            "               '    <div style=\"width: 4%;\">',\r",
                                            "               '        <div style=\"width: 20px;\" data-name=\"all\"></div>',\r",
                                            "               '    </div>',\r",
                                            "               '    <div class=\"rightBorderDiv\" style=\"width: 22%;\" data-name=\"invoice_number\">',\r",
                                            "               '        Invoice No&nbsp;<img src=\"resources/images/button_icons/downArrow.png\"/>',\r",
                                            "               '    </div>',\r",
                                            "               '    <div class=\"rightBorderDiv\" style=\"width: 16%;\" data-name=\"invoice_product\">',\r",
                                            "               '        Product&nbsp;<img src=\"resources/images/button_icons/downArrow.png\"/>',\r",
                                            "               '    </div>',\r",
                                            "               '    <div class=\"rightBorderDiv\" style=\"width: 18%;\" data-name=\"due_date\">',\r",
                                            "               '        Due Date&nbsp;<img src=\"resources/images/button_icons/downArrow.png\"/>',\r",
                                            "               '    </div>',\r",
                                            "               '    <div class=\"rightBorderDiv\" style=\"width: 20%;\" data-name=\"total_payble\">',\r",
                                            "               '        Amount&nbsp;<img src=\"resources/images/button_icons/downArrow.png\"/>',\r",
                                            "               '    </div>',\r",
                                            "               '    <div style=\"width: 16%;\" data-name=\"payment_status\">',\r",
                                            "               '        Status&nbsp;<img src=\"resources/images/button_icons/downArrow.png\"/>',\r",
                                            "               '    </div>',\r",
                                            "               '</div>'\r",
                                            "              )\r",
                                            ");\r",
                                            "headers.refresh();\r",
                                            "var list = component.down('#mainList');\r",
                                            "list.setStore(Ext.getStore('InvoiceHistoryStore'));\r",
                                            "list.setItemTpl(\r",
                                            "    Ext.create('Ext.XTemplate',\r",
                                            "               '<div style = \"height: 20px;padding-top: 0px;\" class=\"x-rm-listtpl-main pointerCursor\">',\r",
                                            "               '    <div style=\"width: 4%;margin-right: 0px;\">',\r",
                                            "               '    </div>',\r",
                                            "               '    <div class=\"rightBorderDiv\" style=\"width: 22%;\">{invoice_number}</div>',\r",
                                            "               '    <div class=\"rightBorderDiv\" style=\"width: 16%;\">{[this.getProductType(values)]}</div>',\r",
                                            "               '    <div class=\"rightBorderDiv\" style=\"width: 18%;padding-right: 2%;text-align: right;\">{due_date:date(\"m/d/Y\")}</div>',\r",
                                            "               '    <div class=\"rightBorderDiv\" style=\"width: 20%;padding-right:2%;text-align: right;\">$ {[formatNum(values.total_payble)]}</div>',\r",
                                            "               '    <div style=\"width: 16%;text-align:center;\">{[this.getPaymentStatus(values)]}</div>',\r",
                                            "               '</div>',\r",
                                            "               {\r",
                                            "                   getProductType:function(record){\r",
                                            "\r",
                                            "                       if(record.invoice_product == \"product_datalink\"){\r",
                                            "\r",
                                            "                           return \"Datalink\" ;\r",
                                            "                       }else if(record.invoice_product == \"product_rmpro\"){\r",
                                            "                           return \"RMPro\" ;\r",
                                            "                       }else if(record.invoice_product == \"product_ecom\"){\r",
                                            "                           return \"E-Commerce\" ;\r",
                                            "                       }\r",
                                            "\r",
                                            "                       return \"\";\r",
                                            "\r",
                                            "                   },\r",
                                            "                   getPaymentStatus:function(record){\r",
                                            "                       if(! record.payment_status){\r",
                                            "                           return \"Paid\" ;\r",
                                            "                       }\r",
                                            "\r",
                                            "                       if( record.payment_status == \"paid\"){\r",
                                            "                           return \"Paid\" ;\r",
                                            "                       }else{\r",
                                            "                           return '<span style=\"color: rgb(157, 8, 8);\">Unpaid </span>' ;\r",
                                            "                       }\r",
                                            "\r",
                                            "                   }\r",
                                            "\r",
                                            "               }\r",
                                            "              )\r",
                                            ");\r",
                                            "\r",
                                            "list.setMode(\"SINGLE\");\r",
                                            "\r",
                                            "list.setAllowDeselect(false);\r",
                                            "\r",
                                            "    list.on(\"select\",function(invoiceList, record, eOpts){\r",
                                            "\r",
                                            "         var currentInvoiceLst = Ext.ComponentQuery.query('#invoiceCurrentSubscriptionsListPanel')[0].down('#mainList');\r",
