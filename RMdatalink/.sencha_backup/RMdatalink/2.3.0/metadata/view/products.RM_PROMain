                                                                    "                     list.deselectAll(true) ;\r",
                                                                    "                     list.select(RMdatalink.app.getController('RMProController').config.prevSelected,true,true);\r",
                                                                    "\r",
                                                                    "\r",
                                                                    "\r",
                                                                    "                }else{\r",
                                                                    "                      RMdatalink.app.getController('RMProController').config.prevSelected = list.getSelection() ;\r",
                                                                    "                      list.selectAll(true) ;\r",
                                                                    "                }\r",
                                                                    "\r",
                                                                    "               RMdatalink.app.getController('RMProController').onRmProSelectUnselect();\r",
                                                                    "         }\r",
                                                                    "\r",
                                                                    "}else{\r",
                                                                    "\r",
                                                                    "    if(dataview.isSelected(record))\r",
                                                                    "    {\r",
                                                                    "\r",
                                                                    "        dataview.deselect(record,true) ;\r",
                                                                    "    }else{\r",
                                                                    "\r",
                                                                    "        dataview.select(record,true) ;\r",
                                                                    "    }\r",
                                                                    "}\r",
                                                                    "\r",
                                                                    "\r",
                                                                    ""
