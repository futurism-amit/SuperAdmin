                                                                "fn": "onRmProListItemTap",
                                                                "implHandler": [
                                                                    "\r",
                                                                    "var attrToSearch = RMdatalink.util.globalConfig.getListAttrForDelHandling();\r",
                                                                    "var targetEl = e.target;\r",
                                                                    "\r",
                                                                    "\r",
                                                                    "// delegateAttr=\"onCartTap\"\r",
                                                                    "\r",
                                                                    "if( RMdatalink.util.globalConfig.isAttributePresentInTarget( attrToSearch,targetEl )  ){\r",
                                                                    "\r",
                                                                    "         var attrVak = targetEl.getAttribute(attrToSearch);\r",
                                                                    "\r",
                                                                    "         if(attrVak == \"onCartTap\")\r",
                                                                    "         {\r",
                                                                    "\r",
                                                                    "             //always before onRmProSelectUnselect\r",
                                                                    "\r",
                                                                    "              RMdatalink.app.getController('RMProController').onRmProSelectionAction(dataview,record,index);\r",
                                                                    "\r",
                                                                    "\r",
                                                                    "                   // var timeout = setTimeout(function(){\r",
                                                                    "\r",
                                                                    "                         RMdatalink.app.getController('RMProController').onRmProSelectUnselect();\r",
                                                                    "\r",
                                                                    "                   //      clearTimeout(timeout);\r",
                                                                    "                    //},100);\r",
                                                                    "\r",
                                                                    "\r",
                                                                    "         }\r",
                                                                    "\r",
                                                                    "}else{\r",
                                                                    "\r",
                                                                    "    if(dataview.isSelected(record))\r",
                                                                    "    {\r",
                                                                    "\r",
                                                                    "        dataview.deselect(record,true) ;\r",
                                                                    "    }else{\r",
                                                                    "\r",
                                                                    "        dataview.select(record,true) ;\r",
                                                                    "    }\r",
                                                                    "\r",
                                                                    "    var str = dataview.getStore();\r",
                                                                    "    for(var i=0; i < str.getData().all.length ; i++){\r",
                                                                    "        str.getAt(i).set('is_itemtap',false);;\r",
                                                                    "    }\r",
                                                                    "     record.set('is_itemtap',true);\r",
                                                                    "\r",
                                                                    "     RMdatalink.app.getController('RMProController').onProductRMProListSelect(dataview, record, eOpts);\r",
                                                                    "}"
                                                                ],
